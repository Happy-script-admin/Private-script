--SYSTEM

local  = "\104\116\116\112\115\58\47\47\114\97\119\46\103\105\116\104"
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local HttpService = game:GetService("HttpService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local loadingGui = playerGui:WaitForChild("HAPPYscriptLoadingScreen")
local frame = loadingGui:WaitForChild("Frame")

local Loading = frame:WaitForChild("Loading")
local Khung = frame:WaitForChild("Khung")
local Logo = frame:WaitForChild("Logo")
local Title = frame:WaitForChild("Title")
local LoadingText = frame:WaitForChild("LoadingText")

local gradient = Logo:FindFirstChildWhichIsA("UIGradient")
local stroke = Khung:FindFirstChildWhichIsA("UIStroke")
local  = "\97\100\115\47\109\97\105\110\47\72\65\80\80\89\95\97\100\109\105\110\95\99\104\97\116"
local frameClosed = UDim2.new(1.1, 0, 0.4, 0)
local frameOpen   = UDim2.new(0.9, 0, 0.4, 0)

local tweenInfoFast = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local tweenInfoSlow = TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)

local function safeTween(obj, props, info)
	info = info or tweenInfoFast
	local ok, tw = pcall(function() return TweenService:Create(obj, info, props) end)
	if not ok or not tw then return end
	tw:Play()
	local ev = tw.Completed
	if ev then ev:Wait() end
end

local function safePcall(f, ...)
	local ok, res = pcall(f, ...)
	return ok, res
end

local function typeWrite(lbl, text, delayTime)
	lbl.Text = ""
	for i = 1, #text do
		lbl.Text = string.sub(text, 1, i)
		task.wait(delayTime)
	end
end

local function lerpColor(percent)
	percent = math.clamp(percent, 0, 1)
	local r = math.floor(255 * (1 - percent))
	local g = math.floor(255 * percent)
	return Color3.fromRGB(r, g, 0)
end

local function setLoadingPercent(percent, tweenTime)
	percent = math.clamp(percent or 0, 0, 1)
	local newSize = UDim2.new(0.575 * percent, 0, 0.1, 0)
	local newColor = lerpColor(percent)

	if Loading then
		local ok, tw = pcall(function()
			return TweenService:Create(Loading, TweenInfo.new(tweenTime or 0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
				Size = newSize,
				BackgroundColor3 = newColor
			})
		end)
		if ok and tw then
			tw:Play()
			tw.Completed:Wait()
			return
		end
		Loading.Size = newSize
		Loading.BackgroundColor3 = newColor
	end
end
local  = "\109\105\110\95\99\104\97\116\47\114\101\102\115\47\104\101"
frame.Position = frameClosed
Logo.Visible = false
Title.Visible = false
LoadingText.Visible = false
Khung.Visible = false
Loading.Visible = false
Loading.Size = UDim2.new(0, 0, 0.1, 0)
Loading.BackgroundColor3 = lerpColor(0)

if stroke then stroke.Transparency = 1 end
if gradient then gradient.Offset = Vector2.new(0, 2) end

safeTween(frame, {Position = frameOpen}, TweenInfo.new(0.55, Enum.EasingStyle.Quad, Enum.EasingDirection.Out))

Logo.Visible = true
if gradient then
	-- faster tween
	safeTween(gradient, {Offset = Vector2.new(0, -2)}, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out))
else
	Logo.Visible = true
end
local titleText = "HAPPY script"
local  = "\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\72\65\80"
Title.Visible = true
Title.Text = ""
typeWrite(Title, titleText, 0.06)
LoadingText.TextTransparency = 1
LoadingText.Visible = true
safeTween(LoadingText, {TextTransparency = 0}, TweenInfo.new(0.4))

--------------------------------------------------------------------------------
-- CALLBACK mà script trong URL sẽ gọi để update tiến độ
--------------------------------------------------------------------------------
shared.HAPPY_LOADER_UPDATE = function(statusText, percent)
	-- đổi chữ
	if typeof(statusText) == "string" then
		LoadingText.Text = statusText
	end

	-- đổi %
	if typeof(percent) == "number" then
		setLoadingPercent(percent, 0.25)
	end
end

Khung.Visible = true
if stroke then safeTween(stroke, {Transparency = 0}, TweenInfo.new(0.45)) end
Loading.Visible = true

local  = "\80\89\45\115\99\114\105\112\116\47\72\65\80\80\89\95\97\100"
local scriptURL =  ..  ..  ..  .. 
local loadedSuccess = false
local fetchTimeout = 30
local function fetchAndRun()
	local ok, body = pcall(function() return game:HttpGet(scriptURL) end)
	if not ok or not body or type(body) ~= "string" then
		return false, "HttpGet failed"
	end
	local ok2, fn = pcall(function() return loadstring(body) end)
	if not ok2 or type(fn) ~= "function" then
		-- loadstring failed
		return false, "loadstring failed"
	end
	local ok3, res = pcall(function() return fn() end)
	-- even if execution errors, count as "loaded" (so loader closes). But we log error.
	if not ok3 then
		warn("[HAPPY loader] external script error:", res)
	end
	return true
end
local co = coroutine.create(function()
	local ok, err = fetchAndRun()
	if ok then loadedSuccess = true else
		for i=1,2 do
			task.wait(0.8)
			local ok2, _ = fetchAndRun()
			if ok2 then
				loadedSuccess = true
				break
			end
		end
	end
end)

coroutine.resume(co)

coroutine.resume(co)

-- Vòng chờ fetch (tối đa fetchTimeout giây)
local waited = 0
while not loadedSuccess and waited < fetchTimeout do
	task.wait(0.1)
	waited += 0.1
end

-- Dù có timeout hay không thì vẫn đóng loader
setLoadingPercent(1, 0.6)

-- Fade out UI và đóng
local fadeInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local tweens = {}

if Logo:IsA("ImageLabel") or Logo:IsA("ImageButton") then
	tweens[#tweens+1] = TweenService:Create(Logo, fadeInfo, {ImageTransparency = 1})
else
	tweens[#tweens+1] = TweenService:Create(Logo, fadeInfo, {BackgroundTransparency = 1})
end

tweens[#tweens+1] = TweenService:Create(Title, fadeInfo, {TextTransparency = 1})
tweens[#tweens+1] = TweenService:Create(LoadingText, fadeInfo, {TextTransparency = 1})
if stroke then 
	tweens[#tweens+1] = TweenService:Create(stroke, fadeInfo, {Transparency = 1}) 
end
tweens[#tweens+1] = TweenService:Create(Loading, fadeInfo, {BackgroundTransparency = 1})

for _, tw in ipairs(tweens) do
	pcall(function() tw:Play() end)
end

if #tweens > 0 then
	local last = tweens[#tweens]
	if last and last.Completed then last.Completed:Wait() end
end

LoadingText.Visible = false

TweenService:Create(frame, tweenInfoFast, {Position = frameClosed}):Play()
task.wait(tweenInfoFast.Time + 0.05)

pcall(function()
	loadingGui:Destroy()
end)
